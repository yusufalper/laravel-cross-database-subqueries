{
    "name": "yusufalper/laravel-cross-database-subqueries",
    "description": "Eloquent cross database compatibility in subqueries",
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^5.6 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/container": "^5.6 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/database": "^5.6 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/events": "^5.6 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.6.x",
        "codacy/coverage": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Hoyvoy\\CrossDatabase\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hoyvoy\\Tests\\" : "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hoyvoy\\CrossDatabase\\CrossDatabaseServiceProvider"
            ]
        }
    }
}
